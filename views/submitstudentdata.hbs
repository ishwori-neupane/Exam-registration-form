{{#each rows}}
<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">Subject</th>
            <th scope="col">Subject Code</th>
            <th scope="col">Credit hours</th>
            <th scope="col">Level ID</th>
            <th scope="col">Program ID</th>
        </tr>
        
    </thead>
    <tbody>

        <tr>
            <td scope="row">{{this.subject}}</td>
            <td scope="row">{{this.subject_code}}</td>
            <td scope="row">{{this.credit_hours}}</td>
            <td scope="row">{{this.level_id}}</td>
            <td scope="row">{{this.program_id}}</td>
        </tr>
    </tbody>
</table>
{{/each}}